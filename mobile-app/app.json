{
  "expo": {
    "name": "Booked",
    "slug": "booked-mobile-app",
    "scheme": "tscmexpo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/adaptive-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#9B51E0"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.template.app",
      "buildNumber": 1,
      "config": {}
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#9B51E0",
        "config": {},
        "package": "com.template.app",
        "versionCode": 1
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.svg"
    },
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}